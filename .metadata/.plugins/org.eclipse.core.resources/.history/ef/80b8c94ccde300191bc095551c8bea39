import java.util.*;
public class exer5 {
	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		
		
		int i, num= 0;
		int a[] = new int[5];
		int b[] = new int[5];
		
		for(i=0; i<5; i++) {
			System.out.println("Insira os valores ");
			a[i] = in.nextInt();
		}
		
		for(i=0; i<11; i++) {
			num++;
			if(a[i]%2 == 0 ) {
				b[i] = num;
				System.out.print(b[i]+",");
			}
		}
		
		
		
		
		
		
		

	}

}
